#!/bin/bash

root_src_dir="$(dirname $0)"
pushd $root_src_dir >/dev/null

./test-chill/make_chill_testcases.sh  chill      examples/chill/testcases      examples/chill/testcases/RIGHTANSWERS
./test-chill/make_chill_testcases.sh  cuda-chill examples/cuda-chill/testcases examples/cuda-chill/testcases/RIGHTANSWERS

autoreconf -if

popd >/dev/null

